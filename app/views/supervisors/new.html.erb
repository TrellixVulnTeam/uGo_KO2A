<%= devise_error_messages! %>
<div class="container">
  <div class="row-fluid">
    <div class="header">

        <%= link_to root_path do %>
          <%= image_tag "header.png", id:"header-logo" %>
        <% end %>        



    </div>
  </div>



  <div class="row-fluid">

    <div class="sign-up">


      <h1 class="page-title">Supervisor Sign up</h1>



      <%= form_for :supervisor do |f| %>
      <%= devise_error_messages! %>

      <div class="col-md-12">


        <div class="field">
          <%= f.label :employee_number %><br />
          <%= f.text_field :employee_number%>
        </div>

        <div class="actions">
          <%= f.submit "Let's go!", class: 'sign-up-done btn btn-primary done' %>
        </div>

      <% end %>
      


    </div>
  </div>
</div>

