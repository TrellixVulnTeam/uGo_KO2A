<h1>Listing Recommendation</h1>
 
<table>
  <tr>
    <th>ID</th>
    <th>Recommendation</th>
  </tr>
 
  <% @recommendation.each do |recommendation| %>
    <tr>
      <td><%= recommendation.id %></td>
      <td><%= recommendation.application_recommendation %></td>
      <td><%= link_to 'Show', recommendation_path(recommendation) %></td>
    </tr>
  <% end %>
</table>